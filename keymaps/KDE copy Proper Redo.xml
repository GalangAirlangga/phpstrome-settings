<keymap version="1" name="KDE copy Proper Redo" parent="Default for KDE">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
</keymap>